	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.2.xsd
			http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.2.xsd"
	default-autowire="byName">

	<bean id="bookListDomainRepository" class="com.poison.resource.domain.repository.BookListDomainRepository"/> 
	<!-- <bean id="fiLmListDomainRepository" class="com.poison.resource.domain.repository.FilmListDomainRepository"/> -->
	<bean id="serializeDomainRepository" class="com.poison.resource.domain.repository.SerializeDomainRepository"/>
