	<!-- 影单相关 -->
	<bean id="movieListDomainRepository" class="com.poison.resource.domain.repository.MovieListDomainRepository"/>
	<bean id="mvCommentDomainrepository" class="com.poison.resource.domain.repository.MvCommentDomainrepository"/>
	<bean id="myMovieDomainRepository" class="com.poison.resource.domain.repository.MyMovieDomainRepository"/> 
	<bean id="mvDomainRepository" class="com.poison.resource.domain.repository.MyMovieDomainRepository"/> 
